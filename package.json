{
  "name": "webdriverio_jasmine_framework",
  "version": "1.0.0",
  "description": "WebdriverIO - Jasmine framework for Web and Mobile Automation",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf package-lock.json && npm install",
    "cleanup": "rmdir /Q /S reports",
    "test": "npx wdio ./automation/web/config/wdio.conf.js",
    "pretest:docker": "docker-compose up -d",
    "test:docker": "npx wdio ./automation/web/config/wdio.docker.conf.js",
    "posttest:docker": "docker-compose down",
    "test:devtools": "npx wdio ./automation/web/config/wdio.devtools.conf.js",
    "pdf": "node ./automation/report/make-pdf.js",
    "generateAllureReport": "allure generate ./reports/allure-results --clean && allure open",
    "test:android": "npx wdio ./automation/mobile/config/wdio.android.conf.js",
    "install-driver:android": "appium driver install uiautomator2",
    "install-driver:ios": "appium driver install xcuitest",
    "test:api": "npx wdio ./automation/api/config/wdio.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thangarajtk/WDIO_Jasmine_Framework.git"
  },
  "keywords": [
    "WebdriverIO",
    "appium",
    "android",
    "native app",
    "api automation",
    "wiremock"
  ],
  "author": "Thangaraj",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@rpii/wdio-html-reporter-pdf": "^7.0.3",
    "@wdio/allure-reporter": "^7.19.5",
    "@wdio/appium-service": "^7.19.5",
    "@wdio/cli": "^7.19.6",
    "@wdio/devtools-service": "^7.19.6",
    "@wdio/jasmine-framework": "^7.19.5",
    "@wdio/junit-reporter": "^7.19.5",
    "@wdio/local-runner": "^7.19.5",
    "@wdio/selenium-standalone-service": "^7.19.5",
    "@wdio/spec-reporter": "^7.19.5",
    "appium": "^2.0.0-beta.33",
    "appium-doctor": "^1.16.0",
    "axios": "^0.27.2",
    "wdio-chromedriver-service": "^7.3.2",
    "wdio-docker-service": "^3.2.0",
    "wdio-html-nice-reporter": "^8.0.0",
    "wdio-timeline-reporter": "^5.1.4",
    "wdio-wiremock-service": "^6.0.7",
    "webdriverio": "^7.19.5"
  }
}
